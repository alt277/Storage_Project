<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="rootNode" alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT" prefHeight="400.0" prefWidth="400.0" style="-fx-background-color: linear-gradient(#fff 0%, #eee 10%, #ddd 90%, #bbb 100%);" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.flamexander.netty.example.client.MainController">
   <children>
   
       <HBox>
      <children>
              <TextField fx:id="tfFileName" focusTraversable="false" prefHeight="25.0" prefWidth="252.0" promptText="Укажите имя файла " HBox.hgrow="ALWAYS" />
              <Button fx:id="Button" onAction="#pressOnDownloadBtn" prefHeight="25.0" prefWidth="105.0" text="Получить" />
              <Button fx:id="Button1" onAction="#pressOnDownloadBtn1" prefHeight="25.0" prefWidth="93.0" text="Отправить" />
      </children>
       </HBox>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="319.0" text="client storage" />
       <ListView fx:id="filesList" VBox.vgrow="ALWAYS" />
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="319.0" text="servrer storage" />
       <ListView fx:id="filesList1" nodeOrientation="LEFT_TO_RIGHT" VBox.vgrow="ALWAYS">
        
      </ListView>
   </children>
</VBox>
